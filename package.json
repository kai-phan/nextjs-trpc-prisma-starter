{
	"name": "strongly-typed",
	"version": "1.0.1",
	"scripts": {
		"dev": "next dev -p 3002",
		"start": "next start",
		"ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
		"test-dev": "start-server-and-test dev 3000 test",
		"test-start": "start-server-and-test start 3000 test",
		"test": "run-s test:*",
		"test-unit": "jest",
		"test-e2e": "playwright test",
		"setup": "run-s deploy-db migrate-db seed-db",
		"docker-use-remote": "docker context use home-server",
		"deploy-db": "run-s docker-use-remote docker-compose up -d postgres postgres-backup redis",
		"deploy:app": "run-s docker-use-remote docker-compose up -d nextjs",
		"seed-db": "prisma db seed",
		"prisma-generate": "prisma generate",
		"prisma-format": "prisma format",
		"migrate-db": "prisma migrate dev",
		"prisma-build": "run-s prisma-format prisma-generate migrate-db",
		"prisma-studio": "prisma studio",
		"reset-db": "docker-compose down --volumes --remove-orphans",
		"lint": "next lint",
		"lint:fix": "eslint . --fix && yarn format",
		"type-check": "tsc --noEmit",
		"format": "prettier --write .",
		"up": "yarn upgrade-interactive",
		"up-latest": "yarn up --latest",
		"postinstall": "prisma generate",
		"pre-commit": "lint-staged",
		"prebuild": "prisma generate",
		"build": "next build",
		"postbuild": "echo I run after the build script",
		"prepare": "husky install"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"dependencies": {
		"@chakra-ui/icons": "2.0.13",
		"@chakra-ui/react": "2.4.2",
		"@chakra-ui/theme-tools": "2.0.14",
		"@emotion/react": "11.10.5",
		"@emotion/styled": "11.10.5",
		"@next-auth/prisma-adapter": "1.0.5",
		"@prisma/client": "4.6.1",
		"@trpc/client": "9.27.4",
		"@trpc/next": "9.27.4",
		"@trpc/react": "9.27.4",
		"@trpc/server": "9.27.4",
		"@uiw/react-md-editor": "3.19.7",
		"babel-plugin-superjson-next": "0.4.5",
		"chakra-ui-steps": "^1.8.0",
		"clsx": "1.2.1",
		"framer-motion": "7.6.15",
		"katex": "0.16.3",
		"mermaid": "9.2.2",
		"next": "12.3.4",
		"next-auth": "4.12.2",
		"next-remove-imports": "1.0.8",
		"next-seo": "^5.14.1",
		"nodemailer": "6.8.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-focus-lock": "2.9.2",
		"react-hook-form": "7.39.6",
		"react-icons": "4.6.0",
		"react-intl": "6.2.1",
		"react-markdown": "8.0.3",
		"react-query": "3.39.2",
		"react-syntax-highlighter": "15.5.0",
		"redis": "4.5.1",
		"rehype-katex": "6.0.2",
		"rehype-stringify": "9.0.3",
		"remark-breaks": "3.0.2",
		"remark-frontmatter": "4.0.1",
		"remark-gfm": "3.0.1",
		"remark-math": "5.1.1",
		"remark-parse": "10.0.1",
		"remarkable": "2.0.1",
		"start-server-and-test": "1.14.0",
		"superjson": "1.11.0",
		"unified": "10.1.2",
		"uuidv4": "6.2.13",
		"ws": "8.11.0",
		"zod": "3.19.1"
	},
	"devDependencies": {
		"@playwright/test": "1.28.1",
		"@trivago/prettier-plugin-sort-imports": "3.4.0",
		"@types/jest": "29.2.3",
		"@types/mermaid": "9.1.0",
		"@types/node": "17.0.45",
		"@types/nodemailer": "6.4.6",
		"@types/react": "18.0.25",
		"@types/ws": "8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.42.1",
		"eslint": "8.28.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-hardcore": "25.1.0",
		"eslint-config-next": "12.3.4",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-sznm": "1.0.1",
		"eslint-plugin-github": "4.4.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.31.11",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-sonarjs": "0.16.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.2",
		"jest": "29.3.1",
		"jest-environment-jsdom": "29.3.1",
		"lint-staged": "13.0.4",
		"npm-run-all": "4.1.5",
		"playwright": "1.28.1",
		"prettier": "2.8.0",
		"prisma": "4.6.1",
		"ts-jest": "29.0.3",
		"ts-node": "10.9.1",
		"typescript": "4.9.3"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"prisma": {
		"seed": "yarn ts-node prisma/seed.ts"
	}
}
